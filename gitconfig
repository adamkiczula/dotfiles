[user]
	name = Ross Paffett
	email = ross@rosspaffett.com
[github]
	user = raws
[core]
	editor = $EDITOR -w
	excludesfile = ~/.gitignore
[alias]
	co = checkout
	ct = checkout -t
	ci = commit -v
	ca = commit -av
	 g = grep -1n
	st = status -sb
[color]
	branch = auto
	diff = auto
	status = auto
[apply]
	whitespace = nowarn
[branch]
	autosetupmerge = true
[push]
	default = upstream
[pretty]
	# Use the "summary" format for LevelUp deploy notes:
	# git log --pretty=summary production..staging
	# 5bb8642 Ross Paffett [#6141] Add API v14 location show endpoint
	summary = format:"%h %cn %s"
[rerere]
	enabled = 1
[url "git@github.com:"]
	pushInsteadOf = github:
	pushInsteadOf = git://github.com/
[url "git://github.com/"]
	insteadOf = github:
[credential]
	helper = osxkeychain
